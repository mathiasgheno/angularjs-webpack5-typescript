{
  "name": "angularjs-webpack5-typescript",
  "version": "1.0.0",
  "description": "Minimal angularjs webpack5 with typescript using components",
  "main": "index.js",
  "scripts": {
    "build:dev": "webpack --config webpack.dev.config.js --bail --progress --profile",
    "build:prod": "webpack --config webpack.prod.config.js --bail --progress --profile",
    "serve": "webpack-dev-server --config webpack.dev.config.js --hot --history-api-fallback --progress",
    "start": "npm run serve"
  },
  "author": "Lukasz -Max- Kokoszka",
  "license": "ISC",
  "devDependencies": {
    "@types/angular": "^1.8.3",
    "@types/angular-mocks": "^1.7.1",
    "@types/angular-ui-router": "^1.1.41",
    "@types/core-js": "^2.5.5",
    "@types/jasmine": "^3.10.1",
    "@types/node": "^16.11.6",
    "@types/webpack-env": "^1.16.3",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "autoprefixer": "^10.4.0",
    "core-js": "^3.19.0",
    "css-loader": "^6.5.0",
    "eslint": "^8.1.0",
    "eslint-loader": "^4.0.2",
    "eslint-webpack-plugin": "^3.1.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.4.3",
    "ng-annotate-loader": "^0.7.0",
    "ng-annotate-patched": "^1.13.0",
    "terser-webpack-plugin": "^5.2.4",
    "ts-loader": "^9.2.6",
    "typescript": "^4.4.4",
    "webpack": "^5.60.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.4.0"
  },
  "dependencies": {
    "angular": "^1.8.2"
  }
}
